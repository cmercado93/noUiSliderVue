{
  "name": "nouislidervue",
  "version": "0.0.0",
  "description": "noUiSliderVue es una capa de VueJS sobre el complemento noUiSlider",
  "main": "./dist/nouislidervue.umd.js",
  "module": "./dist/nouislidervue.mjs",
  "style": "./dist/nouislidervue.css",
  "exports": {
    ".": {
      "require": "./dist/nouislidervue.umd.js",
      "import": "./dist/nouislidervue.mjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run minify:mjs",
    "minify:mjs": "uglifyjs dist/nouislidervue.mjs --compress --mangle --output dist/nouislidervue.mjs"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "nouislider": "^15.7.0",
    "uglify-js": "^3.17.4",
    "vite": "^4.1.4",
    "vue": "^3.2.47"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmercado93/noUiSliderVue.git"
  },
  "author": "Carlos Mercado <cmercado93@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cmercado93/noUiSliderVue/issues"
  },
  "homepage": "https://github.com/cmercado93/noUiSliderVue#readme",
  "files": [
    "dist"
  ]
}
